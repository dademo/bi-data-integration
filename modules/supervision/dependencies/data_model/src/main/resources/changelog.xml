<databaseChangeLog
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <property name="schema_name" value="supervision"/>

    <include
        file="changelog/core/db.changelog.core.root.xml"
        relativeToChangelogFile="true"/>
    <include
        file="changelog/database/db.changelog.database.root.xml"
        relativeToChangelogFile="true"/>
    <include
        file="changelog/filesystem/db.changelog.filesystem.root.xml"
        relativeToChangelogFile="true"/>
    <include
        file="changelog/messaging/db.changelog.messaging.root.xml"
        relativeToChangelogFile="true"/>
</databaseChangeLog>
